{
  "name": "canvasvideo.js",
  "version": "1.0.2",
  "author": "Jean-Vincent Roger - jv.roger@84paris.com",
  "description": "canvasvideo.js is a lightweight library to play inline video with sound on iPhone (Safari).",
  "main": "src/media/CanvasVideo.js",
  "bin": "bin/canvasvideo.min.js",
  "scripts": {
    "bundle": "browserify src/media/CanvasVideo.js --standalone CanvasVideo -o bin/canvasvideo.js",
    "uglify": "uglifyjs bin/canvasvideo.js -m -o bin/canvasvideo.min.js",
    "build": "npm run bundle; npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/84Paris/canvasvideo.js.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/84Paris/canvasvideo.js/issues"
  },
  "homepage": "https://github.com/84Paris/canvasvideo.js",
  "keywords": [
    "canvasvideo",
    "canvas video",
    "inline video",
    "video",
    "canvas",
    "inline",
    "iphone",
    "iphone inline video",
    "ios",
    "video ios",
    "video safari",
    "video canvas",
    "browserify"
  ],
  "devDependencies": {
    "browserify": "^12.0.1",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {}
}
